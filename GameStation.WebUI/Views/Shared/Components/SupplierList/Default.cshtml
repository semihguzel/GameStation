@model GameStation.WebUI.Models.SupplierListViewModel

        <div class="d-flex justify-content-center">
            @foreach (var supplier in Model.Suppliers)
            {
                <div class="p-3 badge badge-dark" style="margin-top:5px; margin-left:5px;"><a asp-controller="Product" asp-action="Index" asp-route-supplierId="@supplier.SupplierID" >@supplier.CompanyName</a></div>
            }
    </div>
